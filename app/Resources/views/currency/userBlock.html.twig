<div>

    {% if app.user is not null %}
        <h2>Welcome {{ app.user.username }}!</h2>
        <p>
            <a href="{{ logout_path('main') }}">Logout</a>
        </p>
    {% else %}
        <h2>Welcome Guest!</h2>
        <p>
            <a href="{{ path('fos_user_security_login') }}">Login</a>
            /
            <a href="{{ path('fos_user_registration_register') }}">Register</a>
        </p>
    {% endif %}


</div>